  position: absolute;
  width: 100%;
  height: 100%;
  left: 0px;
  background-color: #ccc;
}
.floor,.random-floor,.other-floor{
  position: absolute;
  z-index: 1;
  height: 30px;
  background-color: black;
}
.floor{ 
  width: 415px;
  bottom: 0px;
  height: 90px;
}
.person,.person-jump{
  height: 60px;
  width: 60px;
  position: absolute;
  
